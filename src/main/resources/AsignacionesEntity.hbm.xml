<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Model.AsignacionesEntity" table="Asignaciones" schema="gestionpiezas">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="idproveedor">
            <column name="idproveedor" sql-type="int"/>
        </property>
        <property name="idpieza">
            <column name="idpieza" sql-type="int"/>
        </property>
        <property name="idproyecto">
            <column name="idproyecto" sql-type="int"/>
        </property>
        <property name="cantidad">
            <column name="cantidad" sql-type="float" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>